  "license": "MIT",
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@types/jest": "^26.0.21",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^5.5.0"
  }
